diff --git a/rc.local.etc b/rc.local.etc
index 846c447..caa7984 100755
--- a/rc.local.etc
+++ b/rc.local.etc
@@ -31,5 +31,13 @@ fi
 # set printk to 7 to dump kernel message to console
 echo 7 > /proc/sys/kernel/printk
 
+if [ -e /usr/bin/key_event ];then
+        key_event &
+fi
+
+if [ -e /usr/bin/ec_uevent ];then
+        ec_uevent &
+fi
+
 exit 0
 
